it}, value) {\n            commit('SET_SEARCH_VALUE')\n        },*/\n        GET_NEW_USER_TO_VUEX({commit}, user) {\n            commit(\"ADD_NEW_USER\", user)\n        },\n        GET_USER_TO_DELETE({commit}, id) {\n            commit(\"DELETE_USER\", id)\n        }\n    },\n    getters: {\n        USERS(state) {\n            return state.users\n        },\n        USER_BY_ID: state => (id) => {\n            return state.users.find(user => user.id === id)\n        },\n        SEARCH_VALUE(state) {\n            return state.searchValue\n        }\n    }\n})\n"]}]}