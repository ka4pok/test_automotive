            <input id=\"icon_prefix\" type=\"text\" v-model=\"searchValue\">\r\n                <label for=\"icon_prefix\">First Name</label>\r\n                <i class=\"material-icons btn waves-effect waves-light searchClass indigo darken-3 prefix\"\r\n                   @click=\"sortUserBySearchValue(searchValue)\"\r\n                >\r\n                    search\r\n                </i>\r\n            </div>\r\n        </div>\r\n\r\n\r\n        <br>\r\n\r\n        <button @click=\"showPopup\"> Добавить пользователя</button>\r\n        <ul>\r\n            <li v-for='user in users' :key=\"user.id\">\r\n                {{user.first_name}}\r\n                <router-link tag=\"button\" :to=\"'/user/'+user.id\">Подробнее</router-link>\r\n                <button @click=\"deleteUser(user.id)\">Удалить</button>\r\n            </li>\r\n        </ul>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\n    import {mapGetters, mapActions} from 'vuex'\r\n    import Popup from \"../components/Popup\";\r\n\r\n    export default {\r\n        name: \"List\",\r\n        data() {\r\n            return {\r\n                users: null,\r\n                searchValue: null,\r\n                isPopupVisible: false\r\n\r\n            }\r\n        },\r\n        computed: {\r\n            ...mapGetters([\r\n                'USERS',\r\n                'SEARCH_VALUE'\r\n            ]),\r\n        },\r\n        watch: {},\r\n        methods: {\r\n            ...mapActions([\r\n                'GET_NEW_USER_TO_VUEX',\r\n                'GET_USER_TO_DELETE'\r\n            ]),\r\n            sortUserBySearchValue(value) {\r\n                if (value) {\r\n                    this.users = this.USERS\r\n                    this.users = this.users.filter((user) => {\r\n                        return user.first_name.toLowerCase().includes(value.toLowerCase())\r\n                    })\r\n                } else {\r\n                    this.users = this.USERS\r\n                }\r\n            },\r\n            showPopup() {\r\n                this.isPopupVisible = true\r\n            },\r\n            closePopup() {\r\n                this.isPopupVisible = false\r\n            },\r\n            addNewUser(data) {\r\n                this.GET_NEW_USER_TO_VUEX(data)\r\n                this.isPopupVisible = false\r\n            },\r\n            deleteUser(id) {\r\n                this.users = this.users.filter((user, index) => {\r\n                    return user.id !== id\r\n                })\r\n                this.GET_USER_TO_DELETE(id)\r\n            }\r\n        },\r\n        mounted() {\r\n            this.sortUserBySearchValue(this.SEARCH_VALUE)\r\n        },\r\n        components: {\r\n            Popup\r\n        }\r\n    }\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n    .searchClass {\r\n        padding-top: 5px;\r\n        margin-left: 10px;\r\n        padding-right: 10px;\r\n    }\r\n</style>"]}]}